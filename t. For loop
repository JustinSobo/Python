friends = ["Spencer", "Griffin", "Jose", "Kyle", "Jenni", "Timmy"]

for name in friends:
    print(name)

for index in range(8):
    print(index)

for index in range(3, 8):
    print(index)

for index in range(len(friends)):
    print(index)

for index in range(5):
    if index == 0:
        print("First Iteration")
    else:
        print("Not first Iteration")
